<!doctype html>
<html>
<head>
<title>注册</title>
<meta http-equiv="Content-Type"
	content="application/vnd.wap.xhtml+xml; charset=utf-8" />
<meta name="viewport"
	content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black">
<meta name="format-detection" content="telephone=no">
<meta http-equiv="Expires" CONTENT="-1">
<meta http-equiv="Cache-Control" CONTENT="no-cache">
<meta http-equiv="Pragma" CONTENT="no-cache">
<link rel="stylesheet" type="text/css"
	href="$!{domain}/wap/css/global.css">
<link rel="stylesheet" type="text/css"
	href="$!{domain}/wap/css/user.css">
	<link rel="stylesheet" type="text/css" href="$!{domain}/wap/css/indi.css">
</head>
<body>
	<form action="$!domain/weixin/register2.htm" method="post"
		class="registerForm">
		<div class="modify_data">
			<ul>
				<li><a href="javascript:;" class="txt_r clearfix">
				 <span class="lt">税务登记证</span> 
				 <span class="pic">
				     <img id="uploadImage2" src="" style="width: 40px; height: 40px;"></span> 
				 <input type="hidden" name="picURL2" id="picURL2" class="picURL2">
				 <input name="myfiles" type="file" id="myBlogImage2" accept="image/*"
						class="picFile2" placeholder="税务登记证" onchange="ajaxFileUploads(2)" />
				</a></li>
				<li><a href="javascript:;" class="txt_r clearfix"> <span
						class="lt">组织机构代码</span> <span class="pic"><img
							id="uploadImage3" src="" style="width: 40px; height: 40px;"></span> <input type="hidden"
						name="picURL3" id="picURL3" class="picURL3"> <input
						name="myfiles" type="file" id="myBlogImage3" accept="image/*"
						class="picFile3" placeholder="组织机构代码" onchange="ajaxFileUploads(3)" />
				</a></li>
				<li><a href="javascript:;" class="txt_r clearfix">
						<p class="addr">
							<input name="invoice" type="text" class="fp_mes" placeholder="请输入发票信息">
						</p>
				</a></li>
			</ul>
		</div>
		<p class="ct" style="margin-top: 60px;">
			<a href="javascript:register();" class="log_btn"
				style="width: 60%; display: inline-block; margin: 0;">下一步</a> <a
				href="javascript:register();" class="ignor">忽略</a>
		</p>
		<input type="hidden" name="mainID" id="mainID" value="$!mainID">
	</form>
	<script type="text/javascript" src="$!{domain}/wap/js/jquery-1.8.3.min.js"></script>
	<script type="text/javascript" src="$!{mallStatic}/js/ajaxfileupload.js"></script>
	<script type="text/javascript" src="$!{mallStatic}/js/user.js"></script>
	<script type="text/javascript">
		function register() {
			$(".registerForm").submit();
		}
	</script>
</body>
</html>